{
  "name": "Cookies extension",
  "description": "A Vue.js web extension",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "cookies-extension",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js",
      "gapi-client.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://auth.uber.com/login/"
      ],
      "exclude_matches": [
        "*://*/*session*"
      ],
      "js": [
        "login.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://partners.uber.com/p3/payments/statements*"
      ],
      "css": [
        "hidden.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://accounts.uber.com/profile/?state*",
        "*://partners.uber.com/p3/payments/_static*"
      ],
      "js": [
        "redirect.js"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "jquery.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "web_accessible_resources": [
    "jquery.min.js"
  ],
  "permissions": [
    "storage",
    "cookies",
    "notifications",
    "tabs",
    "<all_urls>",
    "management",
    "background",
    "identity",
    "identity.email",
    "https://*.uberdrive.com.ua/*",
    "http://*.uberdrive.com.ua/*",
    "https://backend.uberdrive.com.ua/Options/*",
    "https://accounts.google.com/o/oauth2/token"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "oauth2": {
    "client_id": "524679747484-i5op712raqlkae1hferimdb4h2ltbsc3.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.modify"
    ]
  },
  "key": ""
}